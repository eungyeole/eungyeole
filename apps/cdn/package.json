{
  "name": "cdn",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn vercel login && yarn vercel link",
    "start": "yarn vercel dev",
    "dev": "nodemon --watch src --exec 'ts-node' src/index.ts",
    "copy:public": "cp -r src/public dist/public",
    "deploy": "yarn vercel",
    "deploy:prod": "yarn vercel --prod"
  },
  "dependencies": {
    "@fastify/env": "^4.2.0",
    "@fastify/static": "^6.9.0",
    "@vercel/node": "^2.9.2",
    "fastify": "^4.12.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "pino": "^8.10.0",
    "pino-pretty": "^9.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vercel": "^28.15.3"
  }
}
