datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  title        String
  description  String?
  categories   String[]
  createdAt    String
  thumbnailUrl String?
  content      Json
}
